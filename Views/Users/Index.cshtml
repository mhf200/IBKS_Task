@model List<IBKS.Models.Domain.User>
@{
}
<div style="background:#CBC3E3">
    <div class="text-center ">
        <h1 class=" font-weight-bold border border-4 " style="background:#D8BFD8"> Users </h1>


<table class="table">

    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>Date Of Birth</th>
            <th>Department</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td>@user.salary</td>
                <td>@user.DateOfBirth.ToString("dd-MM-yyyy")</td>
                <td>@user.Department</td>
                <td><a href="Users/View/@user.Id">View</a></td>


            </tr>
        }

    </tbody>
</table>
</div>